{
  "size": 20,
  "query": {
    "bool": {
      "must": [
        {
          "match": {
            "name": {
              "query": "elasticsearch",
              "prefix_length": 5,
              "fuzziness": 1
            }
          }
        },
        {
          "query_string": {
            "query": "name:elasticsearch"
          }
        }
      ],
      "must_not": [
        {
          "term": {
            "name": {
              "value": "meilisearch"
            }
          }
        }
      ]
    }
  }
}